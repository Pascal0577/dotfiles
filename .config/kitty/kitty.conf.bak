shell zsh
background_opacity 0.5
confirm_os_window_close 0
clipboard_control no-ask

# BEGIN_KITTY_THEME
# Catppuccin-Macchiato
include current-theme.conf
# END_KITTY_THEME


# BEGIN_KITTY_FONTS
font_family      family='UbuntuMono Nerd Font Mono' features='+ss01 +ss02'
bold_font        auto
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS
